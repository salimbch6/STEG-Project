<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.newPasswordController">
   <children>
      <Button fx:id="saveButton" layoutX="425.0" layoutY="154.0" mnemonicParsing="false" onAction="#changepasswordOnAction" prefHeight="26.0" prefWidth="90.0" style="-fx-background-color: #1F51FF;" text="Save" textFill="WHITE" />
      <Label layoutX="145.0" layoutY="125.0" prefHeight="18.0" prefWidth="123.0" text="New Password :">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="newPasswordField" layoutX="145.0" layoutY="154.0" prefHeight="26.0" prefWidth="241.0" promptText="New Password" />
      <Label fx:id="errorMsgLabel" alignment="CENTER" layoutX="186.0" layoutY="65.0" prefHeight="18.0" prefWidth="256.0" textFill="RED" />
   </children>
</AnchorPane>
